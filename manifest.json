{
  "manifest_version": 2,
  "name": "Prompt Saver",
  "version": "1.0",
  "description": "Promptly Saves your prompts!",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
